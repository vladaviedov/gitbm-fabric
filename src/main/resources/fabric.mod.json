    "Vladyslav Aviedov"
  ],
  "contact": {
    "homepage": "https://github.com/vladaviedov",
    "issues": "https://github.com/vladaviedov/gitbm-fabric/issues",
    "sources": "https://github.com/vladaviedov/gitbm-fabric"
  },
  "license": "MIT",
  "icon": "assets/getinthebucketmod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
