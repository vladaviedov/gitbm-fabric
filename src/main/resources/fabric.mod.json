{
  "schemaVersion": 1,
  "id": "gitbm",
  "version": "${version}",
  "name": "Get In The Bucket Mod",
  "description": "Mod that allows you to put mobs into buckets",
  "authors": [
    "Vladyslav Aviedov"
  ],
  "contact": {
    "homepage": "https://github.com/vladaviedov",
    "issues": "https://github.com/vladaviedov/gitbm-fabric/issues",
    "sources": "https://github.com/vladaviedov/gitbm-fabric"
  },
  "license": "MIT",
  "icon": "assets/gitbm/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "vladaviedov.gitbm.Main"
      }
    ]
  },
  "mixins": [
    "gitbm.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.11.0+kotlin.2.0.0",
    "minecraft": "1.21.x",
    "java": ">=21"
  }
}
